{
  "author": "Volkov Labs",
  "description": "Template to create a new Grafana panel plugin",
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@grafana/e2e": "^9.5.2",
    "@grafana/e2e-selectors": "^9.5.2",
    "@grafana/eslint-config": "^5.1.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@swc/core": "^1.3.60",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.16.16",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.4",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.6",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.8",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.62.1",
    "sass-loader": "13.2.2",
    "style-loader": "3.3.2",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8.4",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "Apache-2.0",
  "name": "volkovlabs-abc-panel",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -c ./.config/webpack/webpack.config.ts --env development",
    "e2e:update": "npm exec cypress install && npm exec grafana-e2e run --update-screenshots",
    "e2e": "npm exec cypress install && npm exec grafana-e2e run",
    "levitate": "npx @grafana/levitate@latest is-compatible --path src/module.ts --target @grafana/data,@grafana/runtime,@grafana/ui",
    "sign": "npx --yes @grafana/sign-plugin@latest --rootUrls http://localhost:3000/",
    "start": "docker-compose pull && docker-compose up",
    "stop": "docker-compose down",
    "test": "jest --maxWorkers 4",
    "upgrade": "npm upgrade --save",
    "watch": "webpack -w -c ./.config/webpack/webpack.config.ts --env development"
  },
  "version": "2.4.0",
  "dependencies": {
    "@emotion/css": "^11.11.0",
    "@grafana/data": "9.5.2",
    "@grafana/runtime": "9.5.2",
    "@grafana/ui": "9.5.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tslib": "2.5.0"
  }
}
